{
  "name": "react-input-components",
  "version": "0.1.2-0",
  "homepage": "https://MOES-Media.github.io/react-input-components/",
  "scripts": {
    "start": "NODE_PATH=./src react-scripts start",
    "build": "NODE_PATH=./src react-scripts build",
    "test": "NODE_PATH=./src react-scripts test --env=jsdom --setupTestFrameworkScriptFile=./src/__test__/setup.js",
    "test:coverage": "NODE_PATH=./src react-scripts test --env=jsdom --setupTestFrameworkScriptFile=./src/__test__/setup.js --coverage --collectCoverageFrom=src/**/*js --collectCoverageFrom=!src/stories/**/*.js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/types/**/*js",
    "eject": "NODE_PATH=./src react-scripts eject",
    "lint": "eslint --config node_modules/eslint-config-react-app/index.js src",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "storybook": "NODE_PATH=./src start-storybook -p 9009 -s public",
    "build-storybook": "NODE_PATH=./src build-storybook -s public"
  },
  "dependencies": {
    "ionicons": "^3.0.0",
    "polished": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17",
    "styled-components": "^2.4.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-links": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "flow-bin": "^0.62.0",
    "jest-enzyme": "^4.0.1"
  },
  "eslintIgnore": [
    "src/__test__/",
    "src/stories/"
  ]
}
